class Calculator
{
public static void main(String[]args)
{
Calculator calc = new Calculator();
int result = calc.add();
System.out.println(result);
int no1 = 10, no2 = 20;
calc.multiply(no1, no2);//parameter-inputs
float no3 = 10.5f, no4 = 67.5f;
calc.multiply(no3, no4);
calc.multiply(10,20,30);
}
void multiply(int no1, int num2, int num3)
{

System.out.println(no1*num2*num3);

}
void multiply(float no1, float num2)//arguments
{
System.out.println(no1+num2);
}
void multiply(int no1, int num2)//arguments
{
no1 =12;
System.out.println(no1*num2);
}
int add()
{
int no1=5, no2=10;
int output = no1 + no2;
return output;
//returns the control to method calling statement
}
}
